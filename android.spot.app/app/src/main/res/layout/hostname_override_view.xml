<?xml version="1.0" encoding="utf-8"?>
<com.google.frameworks.client.data.android.debug.HostnameOverrideView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="10dp"
    android:layout_marginBottom="10dp"
    android:paddingStart="5dp"
    android:paddingEnd="5dp"
    android:layout_marginStart="5dp"
    android:layout_marginEnd="5dp">
    <TextView
        android:textSize="16sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Prod Hostname"/>
    <TextView
        android:textSize="14sp"
        android:typeface="monospace"
        android:id="@+id/prod_host_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:textSize="16sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Selected Endpoint"/>
    <TextView
        android:textSize="14sp"
        android:typeface="monospace"
        android:id="@+id/endpoint_value"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <TextView
        android:textSize="16sp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="Services"/>
    <TextView
        android:textSize="14sp"
        android:typeface="monospace"
        android:id="@+id/services_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</com.google.frameworks.client.data.android.debug.HostnameOverrideView>
